<p style="color: green"><%= notice %></p>
<%= paginate @addresses, class: "page-link"  %>
<% content_for :title, "Addresses" %>

<h1>Addresses</h1>

<div id="addresses">
  <% @addresses.each do |address| %>
    <%= render address %>
    <p>
      <%= link_to "Show this address", address %>
    </p>
  <% end %>
</div>
</br>
<%= link_to "New address", new_address_path %>
